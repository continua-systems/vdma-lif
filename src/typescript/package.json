  "bugs": {
    "url": "https://github.com/continua-systems/vdma-lif/issues"
  },
  "homepage": "https://github.com/continua-systems/vdma-lif#readme",
  "devDependencies": {
    "@types/chai": "^5.0.1",
    "@types/mocha": "^10.0.9",
    "chai": "^5.1.2",
    "mocha": "^10.8.2",
    "prettier": "^3.3.3",
    "ts-mocha": "^10.0.0",
    "typescript": "^5.6.3"
  }
